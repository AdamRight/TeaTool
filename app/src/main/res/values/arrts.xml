<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TeaTextView">
        <attr name="teaText" format="string"/>
        <attr name="teaTextColor" format="color"/>
        <attr name="teaTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ArcSpeedView">
        <attr name="bottomColor" format="color"/>
        <attr name="topColor" format="color"/>
        <attr name="middleTextColor" format="color"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="middleTestSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ClipTextView">
        <attr name="originColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TeaRatingBar">
        <attr name="starNormal" format="reference"/>
        <attr name="starSelected" format="reference"/>
        <attr name="starNum" format="integer"/>
    </declare-styleable>

</resources>
