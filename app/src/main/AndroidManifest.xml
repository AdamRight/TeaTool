<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tea.teatool">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".keyboard.KeyBoardActivity" />
        <activity android:name=".flow.FlowActivity" />
        <activity android:name=".webshop.WebShopActivity" />
        <activity android:name=".teatextview.TeaTextViewActivity" />
        <activity android:name=".arcspeed.ArcSpeedActivity" />
        <activity android:name=".cliptext.ClipTextActivity" />
        <activity android:name=".tearatingbar.TeaRatingBarActivity" />
        <activity android:name=".letterindex.LetterIndexActivity" />
        <activity android:name=".mazelock.MazeLockActivity" />
        <activity android:name=".loadingshape.ShapeLoadActivity" />
        <activity android:name=".teahandler.TeaHandlerActivity"></activity>
    </application>

</manifest>