        <activity
            android:name=".mazelock.MazeLockActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".loadingshape.ShapeLoadActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".teaeventbus.TeaEventBusActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".teabutterknife.ButterknifeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".verticaldrag.VerticalDragActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".slidingmenu.SlidingMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".tabmenu.TabMenuActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>