        <activity
            android:name=".cliptext.ClipTextActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".tearatingbar.TeaRatingBarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".letterindex.LetterIndexActivity"
